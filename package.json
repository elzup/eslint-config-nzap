{
	"name": "eslint-config-nzap",
	"version": "1.2.5",
	"description": "my eslint config",
	"main": "index.js",
	"repository": "elzup/eslint-config-nzap",
	"author": "elzup",
	"license": "MIT",
	"engines": {
		"node": ">=8"
	},
	"keywords": [
		"eslint",
		"prettier",
		"react",
		"jest",
		"typescript"
	],
	"scripts": {
		"fmt": "prettier --write '*.{js,json,md}'",
		"lint": "eslint -c ./index.js index.js",
		"find-new-rules": "npm-run-all --parallel find-new-rules:*",
		"find-new-rules:jest": "eslint-find-rules --unused -v ./test/jest.js",
		"find-new-rules:ts": "eslint-find-rules --unused -v ./test/with_typescript.js",
		"find-new-rules:main": "eslint-find-rules --unused -v ./index.js",
		"eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
		"test": "yarn lint",
		"test:tsx": "eslint -c ./test/with_tsx.js ./test/test.tsx"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^2.3.2",
		"@typescript-eslint/parser": "^2.3.2",
		"babel-eslint": "^10.0.3",
		"eslint-config-prettier": "^6.3.0",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-jest": "^23.0.0",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^2.1.2"
	},
	"devDependencies": {
		"@types/react": "16.9.9",
		"eslint": "6.5.1",
		"eslint-find-rules": "3.4.0",
		"npm-run-all": "4.1.5",
		"prettier": "1.18.2",
		"react": "16.10.2",
		"typescript": "3.6.4"
	},
	"peerDependencies": {
		"eslint": ">= 3",
		"typescript": ">=3.0.0"
	},
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
