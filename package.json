{
	"name": "eslint-config-nzap",
	"version": "3.2.0",
	"description": "my eslint config",
	"main": "index.js",
	"repository": "elzup/eslint-config-nzap",
	"author": "elzup",
	"license": "MIT",
	"engines": {
		"node": ">=8"
	},
	"keywords": [
		"eslint",
		"prettier",
		"react",
		"jest",
		"typescript"
	],
	"scripts": {
		"fmt": "prettier --write '*.{js,json,md}'",
		"lint": "eslint -c ./index.js index.js",
		"find-new-rules": "npm-run-all --parallel find-new-rules:*",
		"find-new-rules:ts": "eslint-find-rules --unused -v ./rules/with_typescript.js",
		"find-new-rules:main": "eslint-find-rules --unused -v ./index.js",
		"eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
		"test": "eslint -c ./index.js test/*.js",
		"test:tsx": "eslint -c ./rules/with_tsx.js ./test/test.tsx"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^4.6.1",
		"@typescript-eslint/parser": "^4.6.1",
		"babel-eslint": "^10.1.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0"
	},
	"devDependencies": {
		"@types/react": "16.9.55",
		"eslint": "7.12.1",
		"eslint-find-rules": "3.6.1",
		"npm-run-all": "4.1.5",
		"prettier": "2.1.2",
		"react": "17.0.1",
		"typescript": "4.0.5"
	},
	"peerDependencies": {
		"eslint": ">=6.6.0",
		"typescript": ">=3.0.0"
	},
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
